			resets = <0x4 0x3d>;
			power-domains = <0x4>;
			status = "okay";
			pinctrl-0 = <0x14>;
			pinctrl-names = "default";

			camera@3c {
				compatible = "ovti,ov5645";
				reg = <0x3c>;
				clock-names = "xclk";
				clocks = <0x15>;
				clock-frequency = <0x16e3600>;
				vdddo-supply = <0x16>;
				vdda-supply = <0x17>;
				vddd-supply = <0x18>;
				enable-gpios = <0x10 0x10 0x0>;
				reset-gpios = <0x10 0x142 0x1>;

				port {

					endpoint {
						clock-lanes = <0x0>;
						data-lanes = <0x1 0x2>;
						remote-endpoint = <0x19>;
						phandle = <0x45>;
					};
				};
			};
		};

		i2c@10058400 {
			#address-cells = <0x1>;
