/dts-v1/;

/ {
	compatible = "rzpi", "renesas,r9a07g044l2", "renesas,r9a07g044";
	#address-cells = <0x2>;
	#size-cells = <0x2>;
	model = "NovTech RZpi board";

	audio_clk1 {
		compatible = "fixed-clock";
		#clock-cells = <0x0>;
		clock-frequency = <0xac4400>;
		phandle = <0x7>;
	};

	audio_clk2 {
		compatible = "fixed-clock";
		#clock-cells = <0x0>;
		clock-frequency = <0xbb8000>;
		phandle = <0x8>;
	};

	can {
		compatible = "fixed-clock";
		#clock-cells = <0x0>;
		clock-frequency = <0x0>;
		phandle = <0x11>;
	};

	extal {
		compatible = "fixed-clock";
		#clock-cells = <0x0>;
		clock-frequency = <0x16e3600>;
		phandle = <0x24>;
	};

	opp-table-0 {
		compatible = "operating-points-v2";
		opp-shared;
		phandle = <0x5>;

		opp-150000000 {
			opp-hz = <0x0 0x8f0d180>;
			opp-microvolt = <0x10c8e0>;
			clock-latency-ns = <0x493e0>;
		};

		opp-300000000 {
			opp-hz = <0x0 0x11e1a300>;
			opp-microvolt = <0x10c8e0>;
			clock-latency-ns = <0x493e0>;
		};

		opp-600000000 {
			opp-hz = <0x0 0x23c34600>;
			opp-microvolt = <0x10c8e0>;
			clock-latency-ns = <0x493e0>;
		};

		opp-1200000000 {
			opp-hz = <0x0 0x47868c00>;
			opp-microvolt = <0x10c8e0>;
			clock-latency-ns = <0x493e0>;
			opp-suspend;
		};
	};

	cpus {
		#address-cells = <0x1>;
		#size-cells = <0x0>;

		cpu-map {

			cluster0 {

				core0 {
					cpu = <0x1>;
				};

				core1 {
					cpu = <0x2>;
				};
			};
		};

		cpu@0 {
			compatible = "arm,cortex-a55";
			reg = <0x0>;
			device_type = "cpu";
			#cooling-cells = <0x2>;
			next-level-cache = <0x3>;
			enable-method = "psci";
			clocks = <0x4 0x0 0x0>;
			operating-points-v2 = <0x5>;
			phandle = <0x1>;
		};

		cpu@100 {
			compatible = "arm,cortex-a55";
			reg = <0x100>;
			device_type = "cpu";
			next-level-cache = <0x3>;
			enable-method = "psci";
			clocks = <0x4 0x0 0x0>;
			operating-points-v2 = <0x5>;
			phandle = <0x2>;
		};

		cache-controller-0 {
			compatible = "cache";
			cache-unified;
			cache-size = <0x40000>;
			phandle = <0x3>;
		};
	};

	opp-table-1 {
		compatible = "operating-points-v2";
		phandle = <0x26>;

		opp-500000000 {
			opp-hz = <0x0 0x1dcd6500>;
			opp-microvolt = <0x10c8e0>;
		};

		opp-400000000 {
			opp-hz = <0x0 0x17d78400>;
			opp-microvolt = <0x10c8e0>;
		};

		opp-250000000 {
			opp-hz = <0x0 0xee6b280>;
			opp-microvolt = <0x10c8e0>;
		};

		opp-200000000 {
			opp-hz = <0x0 0xbebc200>;
			opp-microvolt = <0x10c8e0>;
		};

		opp-125000000 {
			opp-hz = <0x0 0x7735940>;
			opp-microvolt = <0x10c8e0>;
		};

		opp-100000000 {
			opp-hz = <0x0 0x5f5e100>;
			opp-microvolt = <0x10c8e0>;
		};

		opp-62500000 {
			opp-hz = <0x0 0x3b9aca0>;
			opp-microvolt = <0x10c8e0>;
		};

		opp-50000000 {
			opp-hz = <0x0 0x2faf080>;
			opp-microvolt = <0x10c8e0>;
		};
	};

	psci {
		compatible = "arm,psci-1.0", "arm,psci-0.2";
		method = "smc";
	};

	firmware {

		optee {
			compatible = "linaro,optee-tz";
			method = "smc";
		};
	};

	soc {
		compatible = "simple-bus";
		interrupt-parent = <0x6>;
		#address-cells = <0x2>;
		#size-cells = <0x2>;
		ranges;

		poe3@10049800 {
			compatible = "renesas,rz-poe3", "renesas,r9a07g044-poe3";
			reg = <0x0 0x10049800 0x0 0x18>;
			interrupts = <0x0 0xd6 0x4 0x0 0xd7 0x4 0x0 0xd8 0x4 0x0 0xd9 0x4>;
			interrupt-names = "oei1", "oei2", "oei3", "oei4";
			clocks = <0x4 0x1 0x12>;
			clock-names = "fck";
			resets = <0x4 0x18>;
			power-domains = <0x4>;
			status = "disabled";
		};

		intc_ex@0x110A0000 {
			compatible = "renesas,rzg2l-irqc";
			#interrupt-cells = <0x2>;
			interrupt-controller;
			reg = <0x0 0x110a0000 0x0 0x10000>;
			interrupts = <0x0 0x1 0x4 0x0 0x2 0x4 0x0 0x3 0x4 0x0 0x4 0x4 0x0 0x5 0x4 0x0 0x6 0x4 0x0 0x7 0x4 0x0 0x8 0x4 0x0 0x0 0x4>;
			interrupt-names = "irq0", "irq1", "irq2", "irq3", "irq4", "irq5", "irq6", "irq7", "nmi";
			clocks = <0x4 0x1 0x8>;
			resets = <0x4 0xf>;
			power-domains = <0x4>;
			status = "disabled";
		};

		ssi@10049c00 {
			compatible = "renesas,r9a07g044-ssi", "renesas,rz-ssi";
			reg = <0x0 0x10049c00 0x0 0x400>;
			interrupts = <0x0 0x146 0x4 0x0 0x147 0x1 0x0 0x148 0x1 0x0 0x149 0x1>;
			interrupt-names = "int_req", "dma_rx", "dma_tx", "dma_rt";
			clocks = <0x4 0x1 0x3c 0x4 0x1 0x3d 0x7 0x8>;
			clock-names = "ssi", "ssi_sfr", "audio_clk1", "audio_clk2";
			resets = <0x4 0x32>;
			dmas = <0x9 0x2655 0x9 0x2656>;
			dma-names = "tx", "rx";
			power-domains = <0x4>;
			#sound-dai-cells = <0x0>;
			status = "okay";
			pinctrl-0 = <0xa>;
			pinctrl-names = "default";
			phandle = <0x4a>;
		};

		ssi@1004a000 {
			compatible = "renesas,r9a07g044-ssi", "renesas,rz-ssi";
			reg = <0x0 0x1004a000 0x0 0x400>;
			interrupts = <0x0 0x14a 0x4 0x0 0x14b 0x1 0x0 0x14c 0x1 0x0 0x14d 0x1>;
			interrupt-names = "int_req", "dma_rx", "dma_tx", "dma_rt";
			clocks = <0x4 0x1 0x3e 0x4 0x1 0x3f 0x7 0x8>;
			clock-names = "ssi", "ssi_sfr", "audio_clk1", "audio_clk2";
			resets = <0x4 0x33>;
			dmas = <0x9 0x2659 0x9 0x265a>;
			dma-names = "tx", "rx";
			power-domains = <0x4>;
			#sound-dai-cells = <0x0>;
			status = "okay";
			pinctrl-0 = <0xb>;
			pinctrl-names = "default";

			ports {

				port@0 {
					phandle = <0x4e>;

					endpoint {
						remote-endpoint = <0xc>;
						dai-format = "i2s";
						phandle = <0x1f>;
					};
				};
			};
		};

		ssi@1004a400 {
			compatible = "renesas,r9a07g044-ssi", "renesas,rz-ssi";
			reg = <0x0 0x1004a400 0x0 0x400>;
			interrupts = <0x0 0x14e 0x4 0x0 0x14f 0x1 0x0 0x150 0x1 0x0 0x151 0x1>;
			interrupt-names = "int_req", "dma_rx", "dma_tx", "dma_rt";
			clocks = <0x4 0x1 0x40 0x4 0x1 0x41 0x7 0x8>;
			clock-names = "ssi", "ssi_sfr", "audio_clk1", "audio_clk2";
			resets = <0x4 0x34>;
			dmas = <0x9 0x265f>;
			dma-names = "rt";
			power-domains = <0x4>;
			#sound-dai-cells = <0x0>;
			status = "disabled";
		};

		ssi@1004a800 {
			compatible = "renesas,r9a07g044-ssi", "renesas,rz-ssi";
			reg = <0x0 0x1004a800 0x0 0x400>;
			interrupts = <0x0 0x152 0x4 0x0 0x153 0x1 0x0 0x154 0x1 0x0 0x155 0x1>;
			interrupt-names = "int_req", "dma_rx", "dma_tx", "dma_rt";
			clocks = <0x4 0x1 0x42 0x4 0x1 0x43 0x7 0x8>;
			clock-names = "ssi", "ssi_sfr", "audio_clk1", "audio_clk2";
			resets = <0x4 0x35>;
			dmas = <0x9 0x2661 0x9 0x2662>;
			dma-names = "tx", "rx";
			power-domains = <0x4>;
			#sound-dai-cells = <0x0>;
			status = "disabled";
		};

		spi@1004ac00 {
			compatible = "renesas,r9a07g044-rspi", "renesas,rspi-rz";
			reg = <0x0 0x1004ac00 0x0 0x400>;
			interrupts = <0x0 0x19f 0x4 0x0 0x19d 0x4 0x0 0x19e 0x4>;
			interrupt-names = "error", "rx", "tx";
			clocks = <0x4 0x1 0x59>;
			resets = <0x4 0x49>;
			power-domains = <0x4>;
			num-cs = <0x1>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			status = "disabled";
		};

		spi@1004b000 {
			compatible = "renesas,r9a07g044-rspi", "renesas,rspi-rz";
			reg = <0x0 0x1004b000 0x0 0x400>;
			interrupts = <0x0 0x1a2 0x4 0x0 0x1a0 0x4 0x0 0x1a1 0x4>;
			interrupt-names = "error", "rx", "tx";
			clocks = <0x4 0x1 0x5a>;
			resets = <0x4 0x4a>;
			power-domains = <0x4>;
			num-cs = <0x1>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			status = "okay";
			pinctrl-0 = <0xd>;
			pinctrl-names = "default";
			dmas = <0x9 0x2e99 0x9 0x2e9a>;
			dma-names = "tx", "rx";
		};

		spi@1004b400 {
			compatible = "renesas,r9a07g044-rspi", "renesas,rspi-rz";
			reg = <0x0 0x1004b400 0x0 0x400>;
			interrupts = <0x0 0x1a5 0x4 0x0 0x1a3 0x4 0x0 0x1a4 0x4>;
			interrupt-names = "error", "rx", "tx";
			clocks = <0x4 0x1 0x5b>;
			resets = <0x4 0x4b>;
			power-domains = <0x4>;
			num-cs = <0x1>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			status = "disabled";
		};

		serial@1004b800 {
			compatible = "renesas,scif-r9a07g044";
			reg = <0x0 0x1004b800 0x0 0x400>;
			interrupts = <0x0 0x17c 0x4 0x0 0x17e 0x4 0x0 0x17f 0x4 0x0 0x17d 0x4 0x0 0x180 0x4 0x0 0x180 0x4>;
			interrupt-names = "eri", "rxi", "txi", "bri", "dri", "tei";
			clocks = <0x4 0x1 0x51>;
			clock-names = "fck";
			power-domains = <0x4>;
			resets = <0x4 0x41>;
			status = "okay";
			pinctrl-0 = <0xe>;
			pinctrl-names = "default";
		};

		serial@1004bc00 {
			compatible = "renesas,scif-r9a07g044";
			reg = <0x0 0x1004bc00 0x0 0x400>;
			interrupts = <0x0 0x181 0x4 0x0 0x183 0x4 0x0 0x184 0x4 0x0 0x182 0x4 0x0 0x185 0x4 0x0 0x185 0x4>;
			interrupt-names = "eri", "rxi", "txi", "bri", "dri", "tei";
			clocks = <0x4 0x1 0x52>;
			clock-names = "fck";
			power-domains = <0x4>;
			resets = <0x4 0x42>;
			status = "disabled";
		};

		serial@1004c000 {
			compatible = "renesas,scif-r9a07g044";
			reg = <0x0 0x1004c000 0x0 0x400>;
			interrupts = <0x0 0x186 0x4 0x0 0x188 0x4 0x0 0x189 0x4 0x0 0x187 0x4 0x0 0x18a 0x4 0x0 0x18a 0x4>;
			interrupt-names = "eri", "rxi", "txi", "bri", "dri", "tei";
			clocks = <0x4 0x1 0x53>;
			clock-names = "fck";
			power-domains = <0x4>;
			resets = <0x4 0x43>;
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <0xf>;
			uart-has-rtscts;

			bluetooth {
				compatible = "brcm,bcm43438-bt";
				device-wakeup-gpios = <0x10 0x128 0x1>;
				host-wakeup-gpios = <0x10 0x129 0x1>;
				status = "okay";
			};
		};

		serial@1004c400 {
			compatible = "renesas,scif-r9a07g044";
			reg = <0x0 0x1004c400 0x0 0x400>;
			interrupts = <0x0 0x18b 0x4 0x0 0x18d 0x4 0x0 0x18e 0x4 0x0 0x18c 0x4 0x0 0x18f 0x4 0x0 0x18f 0x4>;
			interrupt-names = "eri", "rxi", "txi", "bri", "dri", "tei";
			clocks = <0x4 0x1 0x54>;
			clock-names = "fck";
			power-domains = <0x4>;
			resets = <0x4 0x44>;
			status = "disabled";
		};

		serial@1004c800 {
			compatible = "renesas,scif-r9a07g044";
			reg = <0x0 0x1004c800 0x0 0x400>;
			interrupts = <0x0 0x190 0x4 0x0 0x192 0x4 0x0 0x193 0x4 0x0 0x191 0x4 0x0 0x194 0x4 0x0 0x194 0x4>;
			interrupt-names = "eri", "rxi", "txi", "bri", "dri", "tei";
			clocks = <0x4 0x1 0x55>;
			clock-names = "fck";
			power-domains = <0x4>;
			resets = <0x4 0x45>;
			status = "disabled";
		};

		serial@1004d000 {
			compatible = "renesas,r9a07g044-sci", "renesas,sci";
			reg = <0x0 0x1004d000 0x0 0x400>;
			interrupts = <0x0 0x195 0x4 0x0 0x196 0x4 0x0 0x197 0x4 0x0 0x198 0x4>;
			interrupt-names = "eri", "rxi", "txi", "tei";
			clocks = <0x4 0x1 0x56>;
			clock-names = "fck";
			power-domains = <0x4>;
			resets = <0x4 0x46>;
			status = "disabled";
		};

		serial@1004d400 {
			compatible = "renesas,r9a07g044-sci", "renesas,sci";
			reg = <0x0 0x1004d400 0x0 0x400>;
			interrupts = <0x0 0x199 0x4 0x0 0x19a 0x4 0x0 0x19b 0x4 0x0 0x19c 0x4>;
			interrupt-names = "eri", "rxi", "txi", "tei";
			clocks = <0x4 0x1 0x57>;
			clock-names = "fck";
			power-domains = <0x4>;
			resets = <0x4 0x47>;
			status = "disabled";
		};

		poega@10048800 {
			compatible = "renesas,poeg-r9a07g044";
			reg = <0x0 0x10048800 0x0 0x4>;
			interrupts = <0x0 0x142 0x4>;
			clocks = <0x4 0x1 0x14>;
			power-domains = <0x4>;
			resets = <0x4 0x1a>;
			status = "okay";
			phandle = <0x38>;
		};

		poegb@10048C00 {
			compatible = "renesas,poeg-r9a07g044";
			reg = <0x0 0x10048c00 0x0 0x4>;
			interrupts = <0x0 0x143 0x4>;
			clocks = <0x4 0x1 0x15>;
			power-domains = <0x4>;
			resets = <0x4 0x1b>;
			status = "okay";
			phandle = <0x39>;
		};

		poegc@10049000 {
			compatible = "renesas,poeg-r9a07g044";
			reg = <0x0 0x10049000 0x0 0x4>;
			interrupts = <0x0 0x144 0x4>;
			clocks = <0x4 0x1 0x16>;
			power-domains = <0x4>;
			resets = <0x4 0x1c>;
			status = "okay";
			phandle = <0x3a>;
		};

		poegd@10049400 {
			compatible = "renesas,poeg-r9a07g044";
			reg = <0x0 0x10049400 0x0 0x4>;
			interrupts = <0x0 0x145 0x4>;
			clocks = <0x4 0x1 0x17>;
			power-domains = <0x4>;
			resets = <0x4 0x1d>;
			status = "okay";
			phandle = <0x3b>;
		};

		can@10050000 {
			compatible = "renesas,r9a07g044-canfd", "renesas,rzg2l-canfd";
			reg = <0x0 0x10050000 0x0 0x8000>;
			interrupts = <0x0 0x1aa 0x4 0x0 0x1ab 0x4 0x0 0x1a6 0x4 0x0 0x1a8 0x4 0x0 0x1ac 0x4 0x0 0x1a7 0x4 0x0 0x1a9 0x4 0x0 0x1ad 0x4>;
			interrupt-names = "g_err", "g_recc", "ch0_err", "ch0_rec", "ch0_trx", "ch1_err", "ch1_rec", "ch1_trx";
			clocks = <0x4 0x1 0x5c 0x4 0x0 0x16 0x11>;
			clock-names = "fck", "canfd", "can_clk";
			assigned-clocks = <0x4 0x0 0x16>;
			assigned-clock-rates = <0x2faf080>;
			resets = <0x4 0x4c 0x4 0x4d>;
			reset-names = "rstp_n", "rstc_n";
			power-domains = <0x4>;
			status = "disabled";
			pinctrl-0 = <0x12 0x13>;
			pinctrl-names = "default";

			channel0 {
				status = "okay";
			};

			channel1 {
				status = "okay";
			};
		};

		vcp4@10820000 {
			compatible = "renesas,vcp4-fcpcs";
			reg = <0x0 0x10820000 0x0 0x200>;
			renesas,#ch = <0x0>;
		};

		vcp4@10800000 {
			compatible = "renesas,vcp4-vcpl4";
			reg = <0x0 0x10800000 0x0 0x200 0x0 0x10810200 0x0 0x200>;
			interrupts = <0x0 0xa1 0x4 0x0 0xa2 0x4>;
			clocks = <0x4 0x1 0x2d 0x4 0x1 0x2e>;
			power-domains = <0x4>;
			resets = <0x4 0x29 0x4 0x2a>;
			renesas,#ch = <0x0>;
			renesas,#fcp_ch = <0x0>;
		};

		isum@10840000 {
			compatible = "renesas,isum";
			reg = <0x0 0x10840000 0x0 0x1f4>;
			interrupts = <0x0 0x9d 0x4 0x0 0x9e 0x4 0x0 0x9f 0x4 0x0 0xa0 0x4>;
