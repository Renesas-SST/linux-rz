		i2c@10058000 {
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			compatible = "renesas,riic-r9a07g044", "renesas,riic-rz";
			reg = <0x0 0x10058000 0x0 0x400>;
			interrupts = <0x0 0x15e 0x4 0x0 0x15c 0x1 0x0 0x15d 0x1 0x0 0x160 0x4 0x0 0x161 0x4 0x0 0x15f 0x4 0x0 0x162 0x4 0x0 0x163 0x4>;
