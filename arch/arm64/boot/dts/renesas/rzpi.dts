			compatible = "renesas,r9a07g044-csi2";
			reg = <0x0 0x10830400 0x0 0xc00>;
			interrupts = <0x0 0xa6 0x4>;
			clocks = <0x4 0x1 0x30 0x4 0x1 0x2f>;
			clock-names = "vclk", "sysclk";
			resets = <0x4 0x2b>;
			reset-names = "cmn_rstb";
			status = "okay";

			ports {
				#address-cells = <0x1>;
				#size-cells = <0x0>;

				port@1 {
					reg = <0x1>;

					endpoint {
						remote-endpoint = <0x44>;
						phandle = <0x43>;
					};
				};

				port {

					endpoint {
						clock-lanes = <0x0>;
						data-lanes = <0x1 0x2>;
						remote-endpoint = <0x45>;
						phandle = <0x19>;
					};
				};
			};
		};
	};

